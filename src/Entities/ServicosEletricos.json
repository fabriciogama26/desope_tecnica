{
  "name": "ServicosEletricos",
  "type": "object",
  "properties": {
    "data_programacao": {
      "type": "string",
      "format": "date",
      "description": "Data da programação do serviço"
    },
    "projeto": {
      "type": "string",
      "description": "Nome ou código do projeto"
    },
    "ordem_servico": {
      "type": "string",
      "description": "Número da ordem de serviço"
    },
    "tipo_processo": {
      "type": "string",
      "enum": [
        "Expansão",
        "Manutenção"        
      ],
      "description": "Tipo do processo executado"
    },
    "caracteristica_processo": {
      "type": "string",
      "description": "Características específicas do processo"
    },
    "municipio": {
      "type": "string",
      "description": "Município onde será executado o serviço"
    },
    "base_operacional": {
      "type": "string",
      "description": "Base operacional responsável"
    },
    "codigo_equipe": {
      "type": "string",
      "description": "Código da equipe designada"
    },
    "tipo_turma": {
      "type": "string",
      "enum": [
        "Linha Morta",
        "Linha Viva",
        "Poda"
      ],
      "description": "Tipo de turma de trabalho"
    },
    "tecnico_responsavel_cliente": {
      "type": "string",
      "description": "Nome do técnico responsável pelo cliente"
    },
    "lda_lsa": {
      "type": "string",
      "description": "Código LDA/LSA"
    },
    "descricao_servicos": {
      "type": "string",
      "description": "Descrição detalhada dos serviços"
    },
    "quantidade": {
      "type": "number",
      "description": "Quantidade de serviços/unidades"
    },
    "valor_servicos": {
      "type": "number",
      "description": "Valor total dos serviços em reais"
    }
  },
  "required": [
    "data_programacao",
    "projeto",
    "ordem_servico",
    "tipo_processo",
    "municipio",
    "quantidade",
    "valor_servicos"
  ]
}